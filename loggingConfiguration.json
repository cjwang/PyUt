{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "pyutSimple": {
            "format": "%(asctime)s.%(msecs)03d %(levelname)s %(module)s: %(message)s",
            "datefmt" : "%H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class":     "logging.StreamHandler",
            "formatter": "pyutSimple",
            "stream":     "ext://sys.stdout"
        },
        "fileHandler": {
            "class":     "logging.FileHandler",
            "formatter": "pyutSimple",
            "filename":  "pyut.log"
        }
    },
    "loggers": {
        "root": {
            "level":     "INFO",
            "propagate": "False"
        },
        "mediator": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "Pyut": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "AppFrame": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "DiagramFrame": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "PyutXmlV8": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "lang": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
         "IoFile": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
         "PyutDocument": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "DlgPyutProperties": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "OglObject": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "plugins": {
            "level":     "INFO",
            "propagate": "False"
        },
        "PluginManager": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "ToPython": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "IOPython": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        }
    }
}
