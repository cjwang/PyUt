{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "pyutSimple": {
            "format": "%(asctime)s.%(msecs)03d %(levelname)s %(module)s: %(message)s",
            "datefmt" : "%H:%M:%S"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class":     "logging.StreamHandler",
            "formatter": "pyutSimple",
            "stream":     "ext://sys.stdout"
        },
        "fileHandler": {
            "class":     "logging.FileHandler",
            "formatter": "pyutSimple",
            "filename":  "pyut.log"
        }
    },
    "loggers": {
        "root": {
            "level":     "ERROR",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.general.mediator": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut": {
            "level":     "INFO",
            "propagate": "False"
        },
        "Pyut": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.PyutPreferences": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.ui.AppFrame": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.ui.UmlFrame": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.MiniOgl.DiagramFrame": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.persistence.PyutXmlV8": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.general.Lang": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.persistencde.IoFile": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.plugins.ToFastEdit": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.plugins.ToTransforms": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.PyutDocument": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
        },
        "org.pyut.dialogs.DlgPyutProperties": {
            "level":     "INFO",
            "handlers":  ["consoleHandler"],
            "propagate": "False"
