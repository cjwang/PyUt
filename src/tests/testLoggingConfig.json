{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
        },
        "testSimple": {
            "format": "%(levelname)s: %(module)s: %(message)s"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "testSimple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "tests": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "PrintCommand": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "org.pyut.plugins.PluginManager": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestPyutUtils": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestPyutField": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestPluginManager": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestHistory": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "__main__": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        }
    }
}
