{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "simple": {
            "format": "%(asctime)s.%(msecs)03d %(levelname)s %(module)s: %(message)s"
        },
        "testSimple": {
            "format": "%(levelname)s: %(module)s: %(message)s"
        }
    },
    "handlers": {
        "consoleHandler": {
            "class": "logging.StreamHandler",
            "formatter": "testSimple",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "root": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "tests": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestAll": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "tests.PrintCommand": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestDTDParser": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestPyutUtils": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestPyutType": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestPyutField": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },

        "TestPluginManager": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestHistory": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "TestIoPython": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "org.pyut.PyutPreferences": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "org.pyut.plugins.PluginManager": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "org.pyut.plugins.dtd.DTDParser": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },
        "org.pyut.plugins.xsd.XSDParser": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        },

        "__main__": {
            "level": "INFO",
            "handlers": [
                "consoleHandler"
            ],
            "propagate": "False"
        }
    }
}
